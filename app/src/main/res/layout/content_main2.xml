<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_main2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.ali.shiva.Main2Activity"
    tools:showIn="@layout/app_bar_main2">
    <android.support.design.widget.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:theme="@style/AppTheme.AppBarOverlay">

    </android.support.design.widget.AppBarLayout>

    <!--<android.support.design.widget.FloatingActionButton-->
    <!--android:id="@+id/fab"-->
    <!--android:layout_width="wrap_content"-->
    <!--android:layout_height="wrap_content"-->
    <!--android:layout_gravity="bottom|end"-->
    <!--android:layout_margin="@dimen/fab_margin"-->
    <!--app:srcCompat="@android:drawable/ic_dialog_email" />-->

    <android.support.v7.widget.Toolbar xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:local="http://schemas.android.com/apk/res-auto"
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"

        android:layout_alignParentTop="true"
        android:background="?attr/colorPrimary"
        android:minHeight="?attr/actionBarSize"
        local:popupTheme="@style/ThemeOverlay.AppCompat.Light"
        local:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="40dp"
        android:id="@+id/arme_terat"
        android:layout_height="wrap_content"
        android:src="@drawable/terat"  />

    <include layout="@layout/content_main"
        android:visibility="visible"
        android:id="@+id/include" />

    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar3"
        android:layout_marginTop="1dp"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/include"
        android:background="?attr/colorPrimary"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <com.example.ali.shiva.CustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="22dp"
                android:padding="3dp"
                android:paddingRight="5dp"
                android:layout_marginLeft="3dp"
                android:layout_alignParentLeft="true"
                android:id="@+id/item_t1"
                android:text="ALI"/>
            <com.example.ali.shiva.CustomTextView
                android:layout_width="wrap_content"
                android:text="ALII"
                android:layout_below="@+id/item_t1"
                android:textSize="20dp"
                android:layout_marginLeft="7dp"
                android:id="@+id/item_t2"
                android:paddingLeft="5dp"
                android:layout_alignParentLeft="true"
                android:layout_height="wrap_content" />
            <com.example.ali.shiva.CustomTextView
                android:layout_width="wrap_content"
                android:text="BBBBB"
                android:textSize="24dp"
                android:padding="3dp"
                android:layout_marginRight="16dp"
                android:id="@+id/item_t3"
                android:layout_alignParentRight="true"
                android:layout_height="wrap_content" />
            <com.example.ali.shiva.CustomTextView
                android:layout_width="wrap_content"
                android:text="CCCCCC"
                android:layout_marginRight="5dp"
                android:paddingRight="3dp"
                android:id="@+id/item_t4"
                android:textSize="14dp"
                android:layout_below="@+id/item_t3"
                android:layout_alignParentRight="true"
                android:layout_height="wrap_content" />
            <com.example.ali.shiva.CustomTextView
                android:layout_width="wrap_content"
                android:text="10"
                android:id="@+id/item_t5"
                android:textSize="36dp"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:background="@drawable/circle_shap"
                android:layout_height="wrap_content" />

        </RelativeLayout>




    </android.support.v7.widget.Toolbar>



</RelativeLayout>
